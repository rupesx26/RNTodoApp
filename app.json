{
  "expo": {
    "name": "todoapp",
    "slug": "MyReactNativeApp",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rupesh.todoapp"
    },
    
    "android": {
      "package": "com.rupesh.todoapp"
    },
    "description": "First react native app",
    "githubUrl": "https://github.com/rupesx26/RNTodoApp.git"
  }
}